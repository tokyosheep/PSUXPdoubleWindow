{
  "id": "com.doubleWindow",
  "name": "doubleWindow",
  "version": "1.0.0",
  "host": [
    {
      "app": "PS",
      "minVersion": "22.0.0"
    }
  ],
  "main": "index.html",
  "manifestVersion": 4,
  "entrypoints": [
    {
      "type": "panel",
      "id": "profile",
      "label": {
        "default": "profile panel"
      },
      "minimumSize": {
        "width": 120,
        "height": 140
      },
      "maximumSize": {
        "width": 1200,
        "height": 10000
      },
      "preferredDockedSize": {
        "width": 150,
        "height": 200
      },
      "preferredFloatingSize": {
        "width": 300,
        "height": 200
      }
    },
    {
      "type": "panel",
      "id": "edit",
      "label": {
        "default": "edit panel"
      },
      "minimumSize": {
        "width": 120,
        "height": 140
      },
      "maximumSize": {
        "width": 1200,
        "height": 10000
      },
      "preferredDockedSize": {
        "width": 150,
        "height": 200
      },
      "preferredFloatingSize": {
        "width": 300,
        "height": 200
      }
    }
  ]
}